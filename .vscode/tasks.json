{
	"version": "0.1.0",
    "command": "tsc",
	"isShellCommand": true,
	"suppressTaskName": true,
	
	"tasks": [
		{
			"taskName": "build",
			"args": ["-p", "."],
			"isBuildCommand": true,
			"problemMatcher": "$tsc",
			"showOutput": "silent"
		},
		{
			"taskName": "test",
			"args": ["-p", "test"],
			"isTestCommand": true,
			"problemMatcher": "$tsc",
			"showOutput": "silent"
		}
	]
}
