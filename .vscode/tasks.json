{
	"version": "0.1.0",
  "command": "npm",
	"isShellCommand": true,
  "args": ["run"],
	"tasks": [
		{
			"taskName": "build",
			"isBuildCommand": true,
			"problemMatcher": "$tsc",
			"showOutput": "silent"
		},
		{
			"taskName": "test",
			"isTestCommand": true,
			"problemMatcher": "$tsc",
			"showOutput": "always"
		}
	]
}
